        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>Use Business Central on Your Mobile Device| Microsoft Docs</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>You can view and edit your Business Central data on your phone or tablet.</source>
