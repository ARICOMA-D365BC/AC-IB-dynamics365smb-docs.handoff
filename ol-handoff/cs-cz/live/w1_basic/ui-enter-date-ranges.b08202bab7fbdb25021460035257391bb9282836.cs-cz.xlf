        </trans-unit>
        <trans-unit id="178" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`t`</ph> or <ph id="ph2">`today`</ph></source>
        </trans-unit>
        <trans-unit id="179" translate="yes" xml:space="preserve">
          <source>today's date</source>
        </trans-unit>
        <trans-unit id="180" translate="yes" xml:space="preserve">
          <source>date range that includes the fourth accounting period, such as <ph id="ph1">`04/01/20..04/30/20`</ph></source>
        </trans-unit>
        <trans-unit id="181" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`w`</ph> or <ph id="ph2">`workdate`</ph></source>
        </trans-unit>
        <trans-unit id="182" translate="yes" xml:space="preserve">
          <source>the working date</source>
        </trans-unit>
        <trans-unit id="183" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`m`</ph> or <ph id="ph2">`Monday`</ph></source>
        </trans-unit>
        <trans-unit id="184" translate="yes" xml:space="preserve">
          <source>Monday of the work date week</source>
        </trans-unit>
        <trans-unit id="185" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`tu`</ph> or <ph id="ph2">`Tuesday`</ph></source>
        </trans-unit>
        <trans-unit id="186" translate="yes" xml:space="preserve">
          <source>Tuesday of the work date week</source>
        </trans-unit>
        <trans-unit id="187" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`sa`</ph> or <ph id="ph2">`Saturday`</ph></source>
        </trans-unit>
        <trans-unit id="188" translate="yes" xml:space="preserve">
          <source>Saturday of the work date week</source>
        </trans-unit>
        <trans-unit id="189" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`s`</ph> or <ph id="ph2">`Sunday`</ph></source>
        </trans-unit>
