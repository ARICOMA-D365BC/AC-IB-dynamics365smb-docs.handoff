        <trans-unit id="191" translate="yes" xml:space="preserve">
          <source>Item Entries</source>
        </trans-unit>
        <trans-unit id="192" translate="yes" xml:space="preserve">
          <source>Item Journals</source>
        </trans-unit>
        <trans-unit id="193" translate="yes" xml:space="preserve">
          <source>In <ph id="ph1">[!INCLUDE [d365fin](includes/d365fin_md.md)]</ph>, the batch for the migrated data is named <bpt id="p1">**</bpt>C5MIGRATE<ept id="p1">**</ept>.</source>
        </trans-unit>
        <trans-unit id="194" translate="yes" xml:space="preserve">
          <source>Stopping Data Migration</source>
        </trans-unit>
        <trans-unit id="195" translate="yes" xml:space="preserve">
          <source>You can stop migrating data by choosing <bpt id="p1">**</bpt>Stop All Migrations<ept id="p1">**</ept>.</source>
        </trans-unit>
        <trans-unit id="196" translate="yes" xml:space="preserve">
          <source>If you do, all pending migrations are also stopped.</source>
        </trans-unit>
